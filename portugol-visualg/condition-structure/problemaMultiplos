algoritmo "problemaMultiplosV002"


var

valor1, valor2 : inteiro

validacaoNumero: logico
inicio

escreval("Digite dois numeros inteiro:")
leia(valor1)
leia(valor2)

se (valor1 % valor2 = 0) ou (valor2 % valor1 = 0) entao
   escreval("São multiplos")
   
   senao
        escreval("Não são multiplos")
   fimse

fimalgoritmo

